    "import math\n",
    "def gcd(a, b):\n",
    "    a = abs(a)\n",
    "    b = abs(b)\n",
    "    while not b == 0:\n",
    "        if a>b:\n",
    "            tmp = a - a//b * b\n",
    "            a = tmp\n",
    "        else:\n",
    "            tmp = b - b//a * a\n",
    "            b = tmp\n",
    "    print(a)\n"
   ]
